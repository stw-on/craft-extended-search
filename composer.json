{
	"name": "stw-on/craft-extended-search",
	"description": "Retrieve additional context for Element searches",
	"type": "craft-plugin",
	"version": "3.0.1",
	"keywords": [
		"craft",
		"cms",
		"craftcms",
		"craft-plugin",
		"extended search"
	],
	"support": {
		"docs": "https://github.com/stwo-on/craft-extended-search/blob/master/README.md",
		"issues": "https://github.com/stw-on/craft-extended-search/issues"
	},
	"license": "MIT",
	"authors": [
		{
			"name": "Timo Schwarzer / Studentenwerk OstNiedersachsen",
			"homepage": "https://stw-on.de"
		},
		{
			"name": "Mustasj",
			"homepage": "https://mustasj.no"
		}
	],
	"require": {
		"craftcms/cms": "^4.0|^5.0"
	},
	"autoload": {
		"psr-4": {
			"stwon\\craftcms\\extendedsearch\\": "src/"
		}
	},
	"extra": {
		"name": "Extended Search",
		"handle": "extended-search",
		"hasCpSettings": false,
		"hasCpSection": false,
		"changelogUrl": "https://raw.githubusercontent.com/stw-on/craft-extended-search/master/CHANGELOG.md",
		"components": {
			"extendedSearchService": "stwon\\craftcms\\extendedsearch\\services\\ExtendedSearchService"
		},
		"class": "stwon\\craftcms\\extendedsearch\\ExtendedSearch"
	},
	"config": {
		"allow-plugins": {
			"yiisoft/yii2-composer": true,
			"craftcms/plugin-installer": true
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require-dev": {
		"craftcms/rector": "dev-main"
	}
}
